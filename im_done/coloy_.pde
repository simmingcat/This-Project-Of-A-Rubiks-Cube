void colored(int COLOR){
  if (COLOR==0){
    fill(0,100,255);
  }
  if (COLOR==1){
    fill(0,150,0);
  }
  if (COLOR==2){
    fill(200,200,0);
  }
  if (COLOR==3){
    fill(200);
  }
  if (COLOR==4){
    fill(255,0,0);
  }
  if (COLOR==5){
    fill(255,150,0);
  }
  if (COLOR==6){
    fill(150);
  }
}
